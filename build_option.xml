<?xml version="1.0" encoding="UTF-8"?>

<project name="option" default="">
    <!-- 项目可配置属性定义 -->
    <property environment="env" />									<!-- 请不要修改 -->
    <property name="project_name" value="${env.JOB_NAME}"/>			<!-- 请不要修改 -->
    <property name="file_name_prefix" value="${env.BaseLine}"/>		<!-- 请不要修改 -->
    <property name="project_path" value="${env.WORKSPACE}"/>		<!-- 请不要修改 -->
    
	<property name="android_home" value="${env.ANDROID_SDK_HOME}"/> <!-- 请不要修改 -->
	<property name="android_tools" value="${android_home}\tools"/>
	<property name="android_platform_tools" value="${android_home}\platform-tools"/>	
	
	<property name="proguard_home" value="${android_tools}\proguard"/>
	
    <property name="keystore" value="${project_path}\debug.keystore"/> <!-- 需要设置签名文件名称-->
    <property name="key_name" value="androiddebugkey"/> 			<!-- 需要设置-->
    <property name="key_pass" value="android"/> 	<!-- 需要设置-->
    <property name="store_pass" value="android"/>

</project>
